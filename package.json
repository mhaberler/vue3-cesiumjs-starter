{
  "name": "flightview3",
  "private": true,
  "version": "0.1.0",
  "type": "module",
  "scripts": {
    "watch": "vue-tsc --watch",
    "dev": "vite",
    "build": "vue-tsc -b && vite build",
    "preview": "vite preview",
    "clean": "bunx rimraf ./dist",
    "deploy-mah": "vite build --base=/apps/$npm_package_name/ && rsync -avz --delete dist/ mah@mah.priv.at:/var/www/static.mah.priv.at/apps/$npm_package_name/ && echo visit https://static.mah.priv.at/apps/$npm_package_name"
  },
  "dependencies": {
    "@tailwindcss/vite": "^4.1.11",
    "@types/cesium": "^1.70.4",
    "@vueuse/core": "^13.5.0",
    "cesium": "^1.131.0",
    "tailwindcss": "^4.1.11",
    "vue": "^3.5.18"
  },
  "devDependencies": {
    "@vitejs/plugin-vue": "^6.0.1",
    "@vue/tsconfig": "^0.7.0",
    "typescript": "~5.8.3",
    "vite": "^7.0.6",
    "vite-plugin-cesium": "^1.2.23",
    "vite-plugin-devtools-json": "^0.3.0",
    "vite-plugin-qrcode": "^0.3.0",
    "vite-plugin-vue-devtools": "^8.0.0",
    "vue-tsc": "^3.0.4"
  },
  "trustedDependencies": [
    "@tailwindcss/oxide",
    "protobufjs"
  ]
}
